  ax_cv_gcc_x86_cpuid_=`cat conftest_cpuid`; rm -f conftest_cpuid
else case e in #(
  e) ax_cv_gcc_x86_cpuid_=unknown; rm -f conftest_cpuid ;;
esac
fi
